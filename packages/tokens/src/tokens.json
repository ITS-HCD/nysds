        "$extensions": {
          "mode": {
            "admin": "{primitive.color.admin-orange.10}",
            "business": "{primitive.color.business-teal.10}",
            "environment": "{primitive.color.environment-green.10}",
            "health": "{primitive.color.health-purple.10}",
